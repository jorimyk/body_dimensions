#%RAML 1.0 DataType
type: object
properties: 
  username:
    type: string
    required: true
  password:
    type: string
    required: true
  email:
    type: string
    required: true
  firstName:
    type: string
    required: false
  lastName:
    type: string
    required: false
  dateOfBirth:
    type: string
    required: false
  gender:
    enum:
    - male
    - female
    required: false
  public:
    type: boolean
    required: false